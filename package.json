{
  "name": "pm2-monyt",
  "description": "PM2 cluster monitoring addon",
  "version": "0.0.0",
  "author": "Minkyu Cho <mrnoname@naver.com>",
  "license": "MIT",
  "homepage": "https://github.com/Pitzcarraldo/pm2-monyt",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "npmName": "pm2-monyt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pitzcarraldo/pm2-monyt.git"
  },
  "bugs": {
    "url": "https://github.com/Pitzcarraldo/pm2-monyt/issues"
  },
  "keywords": [
    "pm2-monyt"
  ],
  "scripts": {
    "clean": "rimraf lib coverage",
    "lint": "eslint src test --fix",
    "test": "mocha",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha",
    "coveralls": "istanbul-coveralls",
    "check": "npm run lint && npm run coverage",
    "build": "npm run check && babel src --out-dir lib --source-maps",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "babel-promisify": "^1.0.0",
    "monyt": "^0.1.0",
    "pm2": "^1.0.1",
    "pmx": "^0.6.0",
    "semver": "^5.1.0"
  },
  "apps": [
    {
      "name": "pm2-monyt",
      "script": "lib/index.js",
      "merge_logs": true,
      "max_memory_restart": "50M"
    }
  ],
  "config": {
    "app": "reduxible-example",
    "host": "localhost",
    "port": "2003"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.7.1",
    "eslint-config-rackt": "^1.1.1",
    "chai": "^3.5.0",
    "isparta": "^4.0.0",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^2.2.5",
    "rimraf": "^2.3.4"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
